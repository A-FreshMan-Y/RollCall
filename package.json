{
  "name": "rollcall-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "description": "Premium Minimalist Roll Call Application",
  "author": "Antigravity",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "electron:build": "npm run build && electron-builder --win",
    "electron:dir": "npm run build && electron-builder --win --dir",
    "electron": "electron ."
  },
  "dependencies": {
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "electron": "^31.0.0",
    "electron-builder": "^26.4.0",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5"
  },
  "build": {
    "appId": "com.rollcall.app",
    "productName": "随机点名",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "nsis",
      "signAndEditExecutable": false
    }
  }
}